{
    "name": "schlessera/test-command",
    "description": "Test command used for testing the package manager.",
    "type": "wp-cli-package",
    "homepage": "http://wp-cli.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alain Schlesser",
            "email": "alain.schlesser@gmail.com",
            "homepage": "https://www.alainschlesser.come",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [ "command.php" ],
        "psr-4": {
            "AlainSchlesser\\TestCommand\\": "src/"
        }
